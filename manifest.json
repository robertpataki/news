  "description": "Block hate mongering news websites",

  "content_scripts": [{
    "js": ["content.js", "jquery-3.1.1.min.js"],
    "css": ["style.css"],
    "matches": ["https://*.google.co.uk/*"],
    "run_at": "document_start"
  }]

}
